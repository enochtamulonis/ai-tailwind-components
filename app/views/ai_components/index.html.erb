<div class="w-full md:p-16">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">Your AI Components</h1>
  </div>

  <div id="ai_components" class="min-w-full">
    <%= render @ai_components.order(created_at: :desc) %>
  </div>
</div>
